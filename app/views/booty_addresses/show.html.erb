<p id="notice"><%= notice %></p>

<div class="span3"></div>
<div class="span6">
  <fieldset>
    <legend>
      Booty Address
    </legend>
    <div class="row">
      <div class="span4">
        <p>
          <strong>
            Street1
          </strong>
        </p>
        <p>
          <%= @booty_address.street1 %>
        </p>
      </div>
      <div class="span2">
        <p>
          <strong>
            Apt Number
          </strong>
        </p>
        <p>
          <%= @booty_address.apt_number %>
        </p>
      </div>
    </div>
    <div class="row">
      <div class="span3">
        <p>
          <strong>
            City
          </strong>
        </p>
        <p>
          <%= @booty_address.city %>
        </p>
      </div>
      <div class="span1">
        <p>
          <strong>
            State
          </strong>
        </p>
        <p>
          <%= @booty_address.state %>
        </p>
      </div>
      <div class="span2">
        <p>
          <strong>
            Zip
          </strong>
        </p>
        <p>
          <%= @booty_address.zip %>
        </p>
      </div>
    </div>
  </fieldset>
</div>
<div class="span3"></div>
